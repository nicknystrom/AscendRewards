<viewdata model="FolderEditModel" />

<ul>
    <li each="var f in Model.Files">
        <span class="filetype-icon ${f.Type.ToString().ToLower()}"></span>
        <span class="filelink">${Html.ActionLink(f.Name, MVC.Public.File.Index(Model.Id, f.Name, null, null))}</span>
        <span class="filedelete"><button>Delete</button></span>
        <span class="filesize">${f.Size}</span>
    </li>
</ul>